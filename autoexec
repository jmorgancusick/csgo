// Grenade Binds
bind z "use weapon_flashbang";bind x "use weapon_smokegrenade";bind c "use weapon_hegrenade";bind v "use weapon_molotov;use weapon_incgrenade"

// Crouch Jump
alias +duckjump "+jump; +duck"; alias -duckjump "-jump; -duck"; bind "SPACE" "+duckjump"

// Scroll Jump
bind mwheelup +jump;bind mwheeldown +jump;bind space +jump

// Inspect Spam
alias "+moveit" "-lookatweapon;+reload"; alias "-moveit" "+lookatweapon;-reload"; bind "f" "+moveit"

// Jump Throw
alias "+jumpthrow" "+jump;-attack;-attack2"; alias "-jumpthrow" "-jump"; bind mouse4 "+jumpthrow"
